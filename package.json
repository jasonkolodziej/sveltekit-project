{
	"name": "sveltekit-project",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"caller": "if [ -e ./yarn.lock ]; then yarn run test:yarn; else npm run test:npm; fi",
		"test": "yarn run test:integration && yarn run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"reestablish": "rm -rf ./node_modules ./.svelte-kit && yarn"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"@types/nodemailer": "^6.4.15",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"carbon-components-svelte": "^0.85.0",
		"carbon-icons-svelte": "^12.6.0",
		"carbon-pictograms-svelte": "^12.10.0",
		"carbon-preprocess-svelte": "^0.11.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"playwright": "^1.43.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"sveltekit-superforms": "^2.13.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"uuid": "^9.0.1",
		"vite": "^5.0.3",
		"vite-test-utils": "^0.6.0",
		"vitest": "^1.5.0"
	},
	"type": "module",
	"packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610",
	"ignoreDeprecations": "5.0",
	"dependencies": {
		"@auth/core": "latest",
		"@auth/d1-adapter": "^1.1.0",
		"@auth/sveltekit": "latest",
		"@types/uuid": "^9.0.8",
		"carbon-components-svelte": "^0.85.0",
		"file-type": "^19.0.0",
		"nodemailer": "^6.9.13",
		"zod": "^3.23.8"
	}
}
